  `fechaventa` date NOT NULL,
  `cliente` varchar(20) DEFAULT NULL,
  `usuario` varchar(20) NOT NULL,
  `medio_pago` varchar(15) NOT NULL,
  `subtotal` int(6) NOT NULL,
  `descuento` int(6) NOT NULL,
  `totalventa` int(6) NOT NULL,

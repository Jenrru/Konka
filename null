DROP TABLE IF EXISTS `tb_salidas`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `tb_salidas` (
  `codsalida` int(10) NOT NULL AUTO_INCREMENT,
  `fecha` date NOT NULL,
  `usuario` varchar(20) NOT NULL,
  `motivo` varchar(20) NOT NULL,
  `nulo` tinyint(1) NOT NULL,
  PRIMARY KEY (`codsalida`),
  KEY `motivo` (`motivo`),
  KEY `usuario` (`usuario`),
  CONSTRAINT `tb_salidas_ibfk_1` FOREIGN KEY (`motivo`) REFERENCES `tb_motivos` (`motivo`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
